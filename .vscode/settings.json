{
    "javascript.format.enable": true,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false,
    "git.ignoreLimitWarning": true,
    "editor.codeActionsOnSave": {
        // For ESLint
        "source.fixAll.eslint": true,
        // For TSLint
        "source.fixAll.tslint": true
    },
}